package example;

public class LoopEx07 {

	public static void main(String[] args) {
//		<문제>ATM[2단계]
//				1. 로그인
//				. 로그인 후 재 로그인 불가
//				. 로그아웃 상태에서만 이용 가능
//				2. 로그아웃
//				. 로그인 후 이용가능
//				3. 서로 다른 두명의 계좌번호와 비밀번호는 아래와 같다.
//				int dbAcc1 = 1111;
//				int dbPw1 = 1234;
//				int dbAcc2 = 2222;
//				int dbPw2 = 2345;

		int dbAcc1 = 1111;
		int dbAcc2 = 2222;
		
		int dbPw1 = 1234;
		int dbPw2 = 2345;


		System.out.println(dbAcc1+"님, 환영합니다.");
		System.out.println(dbAcc2+"님, 환영합니다.");
		System.out.println("로그아웃 됨");
		System.out.println("프로그램 종료");
		
		
		
		


	}

}
